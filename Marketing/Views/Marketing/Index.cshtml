    function Services(Type) {
        $("#DivLeadGeneration, #DivDemandGeneration, #DivDevelopment").hide()
        $(".clsLead, .clsDemand, .clsDevelopment").removeClass('active')
        if (Type == "L") {
            $("#DivLeadGeneration").show()
            $(".clsLead").addClass('active');
        } else if (Type == "D") {
            $("#DivDemandGeneration").show()
            $(".clsDemand").addClass('active');
        } else {
            $("#DivDevelopment").show()
            $(".clsDevelopment").addClass('active');
        }
    }
</script>
