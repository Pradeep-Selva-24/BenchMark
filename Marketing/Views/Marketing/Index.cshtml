<script>
    var invalidChars = [
        "-",
        "+",
        "e",
    ];
    txt_phone.addEventListener("keydown", function (e) {
        if (invalidChars.includes(e.key)) {
            e.preventDefault();
        }
    })
    var FormDetails = [];
    function submitdetails() {
        debugger
        $(".clsaltermsg").html("");
        //var colorcls = sts == 1 ? "green" : sts == 0 ? "red" : "";
        var email = $("#txt_email").val();
        var filter = /^([\w-]+(?:\.[\w-]+)*)@@((?: [\w -] +\.)*\w[\w -]{ 0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i;
        if (!filter.test(email)) {
            $(".clsaltermsg").html("Please provide a valid email address");
            $(".clsaltermsg").addClass("alert-danger");
            email.focus;
            setTimeout(function () {
                $('.clsaltermsg').fadeIn();
            });
            setTimeout(function () {
                $('.clsaltermsg').fadeOut();
            }, 4000);
            return false;
        }
        var param1 = new Date();
        var param2 = param1.getDate() + "" + (param1.getMonth() + 1) + "" + param1.getFullYear() + "" + param1.getHours() + "" + param1.getMinutes() + "" + param1.getSeconds() + "" + param1.getMilliseconds();

        FormDetails.push({
            Refno: "Ref_" + param2,
            Name: $("#txt_name").val(),
            Email: $("#txt_email").val(),
            PhoneNo: $("#txt_phone").val(),
            Website: $("#txt_Website").val(),
            Message: $("#txt_message").val(),
        });
        var Param = {
            _FormDetails: FormDetails
        }
        $.ajax({
            type: "POST",
            url: '/Home/submitdetails',
            contentType: "application/json; charset=utf-8",
            data: JSON.stringify(Param),
            dataType: "json",
            success: function (data) {
                debugger
                if (data.Status == "01") {
                    $(".clsaltermsg").removeClass("alert-danger");
                    $(".clsaltermsg").addClass("alert-success");
                }
                else
                    $(".clsaltermsg").addClass("alert-danger");
                $(".clsaltermsg").html(data.Message);
                setTimeout(function () {
                    $('.clsaltermsg').fadeIn();
                }, 0.000);
                setTimeout(function () {
                    $('.clsaltermsg').fadeOut();
                }, 4000);
                $(".removeval").val("");
            }
        })
    }




